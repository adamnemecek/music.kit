{
  "name": "music.kit-tools",
  "version": "0.0.0",
  "description": "Your javascript music kit",
  "main": "lib/index.js",
  "scripts": {
    "test": "vows test/*.js",
    "docs": "documentation -f md > API.md && documentation -f html -o docs",
    "dist": "browserify lib/index.js | uglifyjs > dist/music.kit.min.js && ls -all dist/",
    "disc": "browserify --full-paths lib/index.js | uglifyjs | discify > tmp/disc.html && open tmp/disc.html",
    "coverage": "istanbul cover vows test/*.js",
    "cover": "npm run coverage && open ./coverage/lcov-report/index.html",
    "benchmarks": "node test/benchmarks/*.js",
    "climate": "npm run coverage && npm run codeclimate",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=fcfefb53aa586b904e50df4da52d6dc294182107e236117758c3a048b17491ce codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danigb/music.kit.git"
  },
  "keywords": [
    "music",
    "note",
    "transpose",
    "interval",
    "gamut",
    "pitchSet",
    "scale",
    "chord",
    "pitch",
    "music.kit"
  ],
  "author": "danigb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/music.kit/issues"
  },
  "homepage": "https://github.com/danigb/music.kit#readme",
  "dependencies": {},
  "devDependencies": {
    "benchmark": "^1.0.0",
    "teoria": "^2.1.1",
    "vows": "^0.8.1"
  }
}
